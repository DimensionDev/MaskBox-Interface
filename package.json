{
  "name": "maskbox-interface",
  "version": "0.1.0",
  "scripts": {
    "dev": "webpack serve --mode=development --port 5000",
    "build": "webpack --mode=production",
    "eslint": "eslint \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier \"src/**/*.{css,less,ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "gen:assets": "node ./scripts/generate-assets.js",
    "gen:icons": "node ./scripts/generate-icons.js",
    "fetch-schema": "node ./scripts/fetch-schema.js",
    "gen:graphql": "graphql-codegen --config codegen.yml"
  },
  "homepage": "https://box.mask.io/",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx,less,md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@gnosis.pm/safe-apps-web3modal": "^5.0.0",
    "@walletconnect/web3-provider": "^1.5.3",
    "classnames": "^2.3.1",
    "date-fns": "^2.25.0",
    "ethereum-blockies": "^0.1.1",
    "ethers": "^5.4.4",
    "ipfs-http-client": "^50.1.2",
    "jotai": "^1.3.8",
    "lodash-es": "^4.17.21",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-hot-toast": "^2.1.0",
    "react-loading-skeleton": "^3.0.1",
    "react-router-dom": "^5.2.0",
    "react-use": "^17.3.1",
    "rss3-next": "^0.6.17",
    "web3modal": "^1.9.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-react-apollo": "^3.1.6",
    "@types/lodash-es": "^4.17.4",
    "@types/prettier": "^2.3.2",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "buffer": "^6.0.3",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "graphql": "^15.6.1",
    "html-webpack-plugin": "^5.3.2",
    "https-browserify": "^1.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "lint-staged": "^11.1.1",
    "mini-css-extract-plugin": "^2.4.4",
    "node-fetch": "^2.6.1",
    "os-browserify": "^0.3.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.3.2",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.6.1-rc",
    "util": "^0.12.4",
    "vite": "^2.4.4",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2",
    "yorkie": "^2.0.0"
  },
  "license": "MIT"
}
